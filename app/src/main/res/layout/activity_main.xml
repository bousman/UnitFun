    android:orientation="vertical"
    tools:context="com.project.bousman.unitfun.UnitActivity">

    <include
        android:id="@+id/app_bar"
        layout="@layout/app_bar" />

    <ListView xmlns:android="http://schemas.android.com/apk/res/android"
